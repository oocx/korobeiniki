    "@angular/platform-browser-dynamic": "~17.3.1",
    "@angular/pwa": "~17.3.1",
    "@angular/router": "~17.3.1",
    "@angular/service-worker": "^17.3.1",
    "firebase": "^10.9.0",
    "lodash-es": "^4.17.21",
    "npm-check-updates": "^16.14.17",
    "rxjs": "7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.4"
